{
  "name": "url-shortener",
  "version": "1.0.9",
  "description": "Express REST API for URL Shortener",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:live",
    "build": "npm run tsc -- --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "start": "node ./build",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "lint": "eslint . --ext .ts",
    "tsc": "./node_modules/typescript/bin/tsc",
    "test": "jest",
    "test-watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "Larry Schirmer",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.0",
    "typescript": "^4.5.5",
    "winston": "^3.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9"
  }
}
